    "chai": "^4.2.0",
    "eslint": "^8.6.0",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
